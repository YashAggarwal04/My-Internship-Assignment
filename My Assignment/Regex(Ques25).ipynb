   "source": [
    "import re\n",
    "\n",
    "def removeDuplicates(text):\n",
    "    regex = r'\\b(\\w+)(?:\\W+\\1\\b)+' \n",
    "    return re.sub(regex, r'\\1', text)\n",
    "\n",
    "text = \"Hello hello world world\"\n",
    "print(removeDuplicates(text))"
   ]
